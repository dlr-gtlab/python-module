${JOBNAME}:
  stage: deploy
  rules:
    - if: $CI_COMMIT_TAG != null
  before_script:
    - git submodule sync
    - git submodule update --init
  script:
    - echo "Hello template"
    - echo $PLATFORMNAME
    - echo $RELEASESTATUS
    - echo $CI_COMMIT_REF_NAME
    - echo $CI_PROJECT_PATH
  needs:
    - project: $CI_PROJECT_PATH
      job: ${DEPENDDEBUG}
      ref: $CI_COMMIT_REF_NAME
      artifacts: true
    - project: $CI_PROJECT_PATH
      job: ${DEPENDRELEASE}
      ref: $CI_COMMIT_REF_NAME
      artifacts: true
  tags:
    - Win10
  variables:
    PLATFORMNAME:  ${PLATFORMNAME}
    RELEASESTATUS: ${RELEASESTATUS}
